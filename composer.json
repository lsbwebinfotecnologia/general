{
  "name": "lsbwebinfo/project",
  "description": "Cronuz app - solução completa para seu negócio",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Licivando Silva",
      "email": "lvando@cronuz.com.br",
      "homepage": "https://www.cronuz.com.br",
      "role": "Developer"
    }
  ],
  "config": {
    "vendor-dir": "vendor"
  },
  "autoload": {
    "psr-4": {
